#include<stdio.h>
int max(int x, int y);

int main()
{
	int a, b, d;
	printf("请输入两个数字：");
	scanf("%d%d", &a, &b);
	d = max(a, b);
	printf("%d\n", d);
	return 0;
}
int max(int x, int y)
{
	if (x > y)
		return x;
	else
		return y;
}
