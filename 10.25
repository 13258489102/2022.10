#include<stdio.h>
int main()
{
	int a, b;
	char ch;
	printf("请输入：");
	
	scanf("%d\n", &a);
	ch=getchar();
	scanf("%d", &b);
	if (ch == '*')
		printf("%d*%d=%d", a, b, a * b);
	else if (ch == '/')
		printf("a/b=%d", a / b);
	else if (ch == '+')
		printf("a+b=%d", a + b);
	else if (ch == '-')
		printf("a-b=%d", a - b);
	else if (ch == '%')
		printf("a%%b=%d", a%b);
	return 0;
}
